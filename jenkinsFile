pipeline {
    
    agent any
    environment {
        PATH = "/usr/share/maven/bin:$PATH"
    }
    stages {
        stage('CHECKOUT GIT') {
            
            steps{
                echo 'Pulling...';
                git branch: 'main',
                url: 'https://github.com/ihebayari1/devops2.git',
                credentialsId: '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';
            }
        }
        stage('BUILD') {
            steps{
                sh 'mvn -f pom.xml clean install'
                }
        }

    }
    
    
}